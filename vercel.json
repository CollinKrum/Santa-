{
  "cleanUrls": true,
  "routes": [
    { "src": "/api/santa-voice", "dest": "/api/santa-voice.js" },
    { "src": "/videos/(.*)", "dest": "/public/videos/$1" },
    { "src": "/(.*\\.(json|mp4|webm))", "dest": "/public/$1" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
